<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="256.0" prefWidth="387.0" spacing="20.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.dba.nvlcheck.NVLCheck">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <AnchorPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="800.0" styleClass="root" stylesheets="@../../../css/style.css">
        <Label fx:id="labelJDK" layoutX="387.0" layoutY="562.0" prefHeight="17.0" prefWidth="355.0" text="JavaSDK" />
        <Label fx:id="labelJavaFX" layoutX="45.0" layoutY="562.0" prefHeight="17.0" prefWidth="269.0" text="JavaFX" />

        <Label fx:id="labelSourceFile" layoutX="190.0" layoutY="90.0" prefHeight="20.0" prefWidth="555.0" />

        <Label fx:id="labelTargetFile" layoutX="190.0" layoutY="143.0" prefHeight="20.0" prefWidth="549.0">
            <font>
                <Font size="14.0" />
            </font></Label>
        <Label fx:id="labelResult" layoutX="42.0" layoutY="186.0" prefHeight="20.0" prefWidth="689.0" textFill="#bc2820">
            <font>
                <Font size="14.0" />
            </font></Label>
        <TableView fx:id="tableDiff" layoutX="42.0" layoutY="218.0" prefHeight="331.0" prefWidth="700.0">
            <columns>
                <TableColumn fx:id="itemCol" prefWidth="116.0" text="Item" />
                <TableColumn fx:id="quantityCol" prefWidth="94.0" text="Quantity" />
                <TableColumn fx:id="skuCol" prefWidth="210.0" text="SKU" />
                <TableColumn fx:id="solIDCol" prefWidth="346.0" text="Solutions ID" />
            </columns>
        </TableView>

        <Button fx:id="buttonCompare" layoutX="521.0" layoutY="36.0" mnemonicParsing="false" onAction="#handleCompare" prefHeight="30.0" prefWidth="87.0" stylesheets="@../../../css/style.css" text="Compare" />
        <Button fx:id="buttonTarget" layoutX="312.0" layoutY="36.0" mnemonicParsing="false" onAction="#handleOpenTargetFile" stylesheets="@../../../css/style.css" text="Select Target">
            <font>
                <Font size="14.0" />
            </font></Button>
        <Button fx:id="buttonQuit" layoutX="682.0" layoutY="36.0" mnemonicParsing="false" onAction="#handleQuit" stylesheets="@../../../css/style.css" text="Quit" />
        <RadioButton fx:id="radio72GB300" layoutX="45.0" layoutY="30.0" mnemonicParsing="false" onAction="#sourceSelect" selected="true" stylesheets="@../../../css/style.css" text="NVL72 GB300">
            <toggleGroup>
                <ToggleGroup fx:id="confGroup" />
            </toggleGroup>
            <font>
                <Font size="14.0" />
            </font>
        </RadioButton>
        <RadioButton fx:id="radio72GB200" layoutX="45.0" layoutY="70.0" mnemonicParsing="false" onAction="#sourceSelect" stylesheets="@../../../css/style.css" text="NVL72 GB200" toggleGroup="$confGroup">
            <font>
                <Font size="14.0" />
            </font></RadioButton>
        <RadioButton fx:id="radio4GB200" layoutX="45.0" layoutY="110.0" mnemonicParsing="false" onAction="#sourceSelect" stylesheets="@../../../css/style.css" text="NVL4 GB200" toggleGroup="$confGroup">
            <font>
                <Font size="14.0" />
            </font></RadioButton>
        <RadioButton fx:id="radio72VR200" layoutX="45.0" layoutY="150.0" mnemonicParsing="false" onAction="#sourceSelect" stylesheets="@../../../css/style.css" text="NVL72 VR200" toggleGroup="$confGroup">
            <font>
                <Font size="14.0" />
            </font></RadioButton>

    </AnchorPane>
</VBox>
